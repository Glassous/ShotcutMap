        {
          "category": "基本导航",
          "items": [
            { "keys": "Ctrl + T", "description": "打开新标签页" },
            { "keys": "Ctrl + Shift + T", "description": "撤销关闭标签页" },
            { "keys": "Ctrl + W", "description": "关闭标签页" },
            { "keys": "Ctrl + Shift + W", "description": "关闭窗口" },
