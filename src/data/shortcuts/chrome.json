  "systems": {
    "windows": {
      "name": "Windows",
      "shortcuts": [
        {
          "category": "标签页和窗口",
          "items": [
            { "keys": "Ctrl + T", "description": "打开新标签页" },
            { "keys": "Ctrl + Shift + T", "description": "重新打开最近关闭的标签页" },
            { "keys": "Ctrl + W", "description": "关闭当前标签页" },
            { "keys": "Ctrl + Shift + W", "description": "关闭当前窗口" },
            { "keys": "Ctrl + N", "description": "打开新窗口" },
            { "keys": "Ctrl + Shift + N", "description": "打开新的无痕窗口" },
            { "keys": "Ctrl + Tab", "description": "切换到下一个标签页" },
            { "keys": "Ctrl + Shift + Tab", "description": "切换到上一个标签页" },
            { "keys": "Ctrl + 1-8", "description": "切换到指定位置的标签页" },
            { "keys": "Ctrl + 9", "description": "切换到最后一个标签页" },
            { "keys": "Alt + F4", "description": "关闭Chrome" }
          ]
        },
        {
          "category": "导航",
          "items": [
            { "keys": "Alt + ←", "description": "后退" },
            { "keys": "Alt + →", "description": "前进" },
            { "keys": "F5", "description": "重新加载页面" },
            { "keys": "Ctrl + F5", "description": "强制重新加载页面" },
            { "keys": "Ctrl + R", "description": "重新加载页面" },
            { "keys": "Ctrl + Shift + R", "description": "强制重新加载页面" },
            { "keys": "Ctrl + D", "description": "将当前页面加为书签" },
            { "keys": "Ctrl + Shift + D", "description": "将所有打开的标签页加为书签" },
            { "keys": "Ctrl + L", "description": "跳转到地址栏" },
            { "keys": "Alt + D", "description": "跳转到地址栏" },
            { "keys": "Ctrl + K", "description": "从地址栏搜索" },
            { "keys": "Ctrl + E", "description": "从地址栏搜索" }
          ]
        },
        {
          "category": "页面功能",
          "items": [
            { "keys": "Ctrl + F", "description": "在页面中查找" },
            { "keys": "Ctrl + G", "description": "查找下一个" },
            { "keys": "Ctrl + Shift + G", "description": "查找上一个" },
            { "keys": "F3", "description": "查找下一个" },
            { "keys": "Shift + F3", "description": "查找上一个" },
