            { "keys": "Ctrl + Shift + O", "description": "打开书签管理器" },
            { "keys": "Ctrl + U", "description": "查看页面源代码" },
            { "keys": "F12", "description": "打开开发者工具" },
            { "keys": "Ctrl + Shift + I", "description": "打开开发者工具" },
            { "keys": "Ctrl + Shift + J", "description": "打开开发者工具" }
