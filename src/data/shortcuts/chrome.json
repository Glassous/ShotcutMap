        {
          "category": "缩放",
          "items": [
            { "keys": "Ctrl + +", "description": "放大页面" },
            { "keys": "Ctrl + -", "description": "缩小页面" },
            { "keys": "Ctrl + 0", "description": "恢复默认缩放级别" },
            { "keys": "F11", "description": "开启全屏模式" }
          ]
